{
  "name": "sbt-backend",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "watch": "tsc -w",
    "dev": "nodemon dist/src/index.js",
    "setupProjekt": "mkdir images && mkdir images\\s images\\u && pnpx prisma migrate dev --name \"init\"",
    "build": "tsc",
    "start": "pnpm dist/src/index.js"
  },
  "dependencies": {
    "@prisma/client": "6.3.1",
    "@prisma/extension-optimize": "^1.1.4",
    "@prisma/instrumentation": "^6",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "4.21.2",
    "jsonwebtoken": "^9.0.2",
    "memoizee": "^0.4.17",
    "uuid": "^11.1.0",
    "validate-image-type": "^3.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "5.0.0",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/memoizee": "^0.4.11",
    "@types/multer": "^1.4.12",
    "@types/node": "22.13.1",
    "multer": "1.4.5-lts.1",
    "nodemon": "^3.1.9",
    "prisma": "6.3.1",
    "ts-node": "10.9.2",
    "typescript": "5.7.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "bcrypt",
      "es5-ext",
      "prisma"
    ]
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  }
}
